<Page x:Class="Practica6.Pages.PageIn"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Practica6.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageIn">

    <Grid>
        <StackPanel>
            <StackPanel>
                <TextBlock Text="Авторизация" Style="{StaticResource TBStyle}"/>
            </StackPanel>
            <StackPanel>
                <GroupBox Header="Введите логин">
                    <TextBox Name="Login" Text="admin"/>
                    
                </GroupBox>
                <GroupBox Header="Введите пароль">
                    <PasswordBox Name="Password" Password="QWERqwer123!@#"/>
                   
                </GroupBox>
            </StackPanel>
            <Button Name="btn_In"  Content="Войти"  Click="btn_In_Click" Style="{StaticResource btnstyle1}"/>
            <Button Name="btn_back" Content="Назад" Click="btn_back_Click" Style="{StaticResource btnstyle1}"/>
        </StackPanel>
    </Grid>
</Page>
